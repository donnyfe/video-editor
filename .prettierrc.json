{
	"$schema": "https://json.schemastore.org/prettierrc",
	"semi": false,
	"tabWidth": 2,
	"singleQuote": true,
	"printWidth": 100,
	"trailingComma": "all",
	"singleAttributePerLine": true,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"jsxSingleQuote": true,
	"bracketSameLine": false,
	"vueIndentScriptAndStyle": true
}
